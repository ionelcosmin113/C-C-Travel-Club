package proiect.fis.CC.controller;

import javafx.fxml.FXML;
import javafx.fxml.FXMLLoader;
import javafx.scene.Node;
import javafx.scene.layout.AnchorPane;

public class ManagerController {
    @FXML
    private AnchorPane go;

    public ManagerController() {
    }

    @FXML
    void AddRooms() {
        try {
            AnchorPane pane = (AnchorPane)FXMLLoader.load(this.getClass().getClassLoader().getResource("AddRooms.fxml"));
            this.go.getChildren().setAll(new Node[]{pane});
        } catch (Exception var2) {
            System.out.println("Cant load the window");
        }

    }

    @FXML
    void DeleteRooms() {
        try {
            AnchorPane pane = (AnchorPane)FXMLLoader.load(this.getClass().getClassLoader().getResource("DeleteRooms.fxml"));
            this.go.getChildren().setAll(new Node[]{pane});
        } catch (Exception var2) {
            System.out.println("Cant load the window");
        }

    }

    @FXML
    void Logout() {
        try {
            AnchorPane pane = (AnchorPane)FXMLLoader.load(this.getClass().getClassLoader().getResource("Login.fxml"));
            this.go.getChildren().setAll(new Node[]{pane});
        } catch (Exception var2) {
            System.out.println("Cant load the window");
        }

    }

    @FXML
    void Request() {
        try {
            AnchorPane pane = (AnchorPane)FXMLLoader.load(this.getClass().getClassLoader().getResource("Request.fxml"));
            this.go.getChildren().setAll(new Node[]{pane});
        } catch (Exception var2) {
            System.out.println("Cant load the window");
        }

    }
}
